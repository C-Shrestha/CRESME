    <input type="hidden" name="NumImage9Clicks" value="@Model.NumImage9Clicks " />

    <button type="submit" class="btn btn-md btn-primary">
        <i class="fa-solid fa-file-pdf"></i>
       Create PDF
    </button>
</form>

